  "dependencies": {
    "@ryanar/react-auth-provider": "^0.3.3",
    "bcrypt": "^5.0.1",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "electron": "^13.1.4",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "express-http-proxy": "^1.6.2",
    "jquery": "^3.6.0",
    "mongoose": "^5.12.14",
    "path": "^0.12.7",
